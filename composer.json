{
    "name" : "seffeng/laravel-sms",
    "description" : "PHP laravel extension sms",
    "keywords" : [
        "PHP",
        "laravel",
        "yii2",
        "seffeng",
        "sms"
    ],
    "license" : "MIT",
    "homepage" : "https://github.com/seffeng/laravel-sms",
    "authors" : [{
            "name" : "seffeng",
            "email" : "save.zxf@gmail.com",
            "homepage" : "https://github.com/seffeng"
        }
    ],
    "require" : {
        "php" : ">=7.1.3",
        "illuminate/support" : "^5.5|^6.0|^7.0|^8.0",
        "illuminate/contracts" : "^5.5|^6.0|^7.0|^8.0",
        "seffeng/sms" : "^1.0"
    },
    "require-dev" : {
        "laravel/framework" : "^5.5|^6.0|^7.0|^8.0",
        "phpunit/phpunit" : "^9.1"
    },
    "autoload" : {
        "psr-4" : {
            "Seffeng\\LaravelSms\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Seffeng\\LaravelSms\\Tests\\" : "tests/"
        }
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Seffeng\\LaravelSms\\SmsServiceProvider"
            ]
        }
    },
    "minimum-stability" : "stable"
}